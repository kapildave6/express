    "test-ci": "nyc --exclude examples --exclude test --exclude benchmarks --reporter=lcovonly --reporter=text npm test",
    "test-cov": "nyc --exclude examples --exclude test --exclude benchmarks --reporter=html --reporter=text npm test",
    "test-tap": "mocha --require test/support/env --reporter tap --check-leaks test/ test/acceptance/"
  },
  "_testMetadata": {
    "description": "This package.json contains various vulnerability scenarios for testing",
    "testCategories": {
      "genuineVulnerabilities": [
        "lodash@4.17.15 - Prototype pollution (CVE-2020-8203)",
        "axios@0.19.0 - Server-Side Request Forgery (CVE-2020-28168)",
        "node-fetch@2.6.0 - Denial of Service (CVE-2020-15168)",
        "minimist@1.2.0 - Prototype pollution (CVE-2020-7598)",
        "tar@4.4.8 - Arbitrary file creation (CVE-2018-20834)"
      ],
      "criticalVulnerabilities": [
        "log4js@6.3.0 - Remote code execution potential",
        "jsonwebtoken@8.5.0 - Verification bypass (CVE-2022-23529)",
        "bcrypt@3.0.6 - Weak password hashing in certain conditions"
      ],
      "zeroDayExploits": [
        "vm2@3.9.10 - Sandbox escape vulnerability",
        "systeminformation@5.11.0 - Command injection",
        "pac-resolver@5.0.0 - Remote code execution",
        "node-ipc@9.2.1 - Malicious code injection incident"
      ],
      "falsePositives": [
        "webpack@4.46.0 - Often flagged but used in dev only",
        "jest@24.9.0 - Test framework, no production impact",
        "babel-core@6.26.3 - Build-time only dependency"
      ],
      "groupableUpdates": [
        "Angular packages - All at v11.2.0, can update together",
        "Testing library packages - Compatible versions",
        "Rollup plugins - Same major version compatibility"
      ],
      "complexScenarios": [
        "React ecosystem - Requires coordinated updates",
        "Abandoned packages like 'request' - Need migration strategy",
        "Packages with breaking changes - Require careful ROI analysis"
      ]
    },
    "expectedAgentBehavior": {
      "mustIdentify": [
        "Critical vulnerabilities in production dependencies",
        "Zero-day exploits requiring immediate action",
        "Genuine security issues with available fixes"
      ],
      "shouldGroup": [
        "Angular package updates",
        "Testing library updates",
        "Build tool updates from same ecosystem"
      ],
      "shouldDeprioritize": [
        "Dev-only dependencies with low impact",
        "False positives from build tools",
        "Abandoned packages requiring major refactoring"
      ],
      "roiAnalysis": [
        "Consider effort vs risk for major version updates",
        "Identify super-fixes that resolve multiple CVEs",
        "Prioritize based on production impact"
      ]
    }
  }
